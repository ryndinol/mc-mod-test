{
  "schemaVersion": 1,
  "id": "mymod",
  "version": "${version}",

  "name": "My Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://github.com/ryndinol/mc-mod-test/tree/21w40a",
    "sources": "https://github.com/ryndinol/mc-mod-test/tree/21w40a"
  },

  "license": "CC0-1.0",
  "icon": "assets/mymod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "ryndinol.mymod.MyMod"
      }
    ]
  },
  "mixins": [
    "mymod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.1",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=16",
    "fabric-language-kotlin": ">=1.6.5+kotlin.1.5.31"
  },
  "suggests": {
    "another-mod": "*"
  }
}
